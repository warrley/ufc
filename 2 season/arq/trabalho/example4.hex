0000 00B4 MOV R0, #11           // R0 = 0xB
0001 A002 LDR R10, [R0, #0]     // R10 = 0xF000
0002 1A22 LDR R1, [R10, #2]     // R1 <= IN1
0003 2A22 LDR R2, [R10, #2]     // R2 <= IN2
0004 3125 ADD R3, R1, R2        // R3 = IN1 + IN2
0005 3A33 STR R3, [R10, #3]     // OUT <= R3
0006 030D CMP R3, R0            // R3 < 11? 
0007 BFA1 JLT #-6               // Yes? Repeat!
0008 9414 MOV R9, #0x41         // No? R9 = 'A'
0009 1A93 STR R9, [R10, #1]     // Print 'A'
000A FFFF HALT                  // Stops CPU!
000B F000 IO_BASE